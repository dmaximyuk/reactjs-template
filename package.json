{
  "name": "reactjs-template",
  "description": "A simple React.js template for everyone!",
  "homepage": ".",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "Dmitry Maximyuk",
    "email": "d_maksimyk@vk.com",
    "url": "https://github.com/dmaximyuk"
  },
  "keywords": [
    "react",
    "template",
    "boilerplate",
    "fsd",
    "feature-sliced",
    "typescript",
    "javascript",
    "sass",
    "swc"
  ],
  "engines": {
    "node": ">=24.12"
  },
  "scripts": {
    "dev": "vite --host",
    "test": "jest",
    "build": "tsc --noEmit && vite build",
    "build:compress": "tsc --noEmit && BUILD_COMPRESS=true vite build",
    "build:analyzer": "BUILD_ANALYZE=true vite build",
    "build:compress-analyzer": "BUILD_COMPRESS=true BUILD_ANALYZE=true vite build",
    "serve": "vite preview",
    "deploy": "gh-pages -d ./dist",
    "update": "ncu -u && npm install",
    "postinstall": "husky",
    "prepare": "husky",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.151.6",
    "@tanstack/react-router-devtools": "^1.151.6",
    "clsx": "^2.1.1",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "ky": "^1.14.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.91.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unused-imports": "^4.3.0",
    "gh-pages": "^6.3.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lightningcss": "^1.30.2",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.3.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "sass": "^1.97.2",
    "sharp": "^0.34.5",
    "steiger": "^0.5.11",
    "svgo": "^4.0.0",
    "terser": "^5.46.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-bundle-analyzer": "^1.3.2",
    "vite-imagetools": "^9.0.2",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-image-optimizer": "^2.0.3",
    "vite-plugin-minify": "^2.1.0",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^6.0.4"
  }
}
