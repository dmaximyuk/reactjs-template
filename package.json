{
  "name": "reactjs-template",
  "description": "A simple React.js template for everyone!",
  "homepage": ".",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "Dmitry Maximyuk",
    "email": "d_maksimyk@vk.com",
    "url": "https://github.com/dmaximyuk"
  },
  "keywords": [
    "react",
    "template",
    "boilerplate",
    "fsd",
    "feature-sliced",
    "typescript",
    "javascript",
    "sass",
    "swc"
  ],
  "engines": {
    "node": ">=22.17"
  },
  "scripts": {
    "dev": "vite --host",
    "test": "jest",
    "build": "vite build",
    "build:insertcss": "vite build --mode insertcss",
    "build:insertcss-compress": "vite build --mode insertcss-compress",
    "build:compress": "vite build --mode compress",
    "build:analyzer": "vite build --mode analyzer",
    "serve": "vite preview",
    "deploy": "gh-pages -d ./dist",
    "update": "ncu -u && npm install",
    "postinstall": "husky",
    "prepare": "husky",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.130.1",
    "@tanstack/react-router-devtools": "^1.130.1",
    "@tanstack/react-store": "^0.7.3",
    "clsx": "^2.1.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "ky": "^1.8.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.32.0",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@tanstack/router-plugin": "^1.130.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.1.2",
    "npm-check-updates": "^18.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "sharp": "^0.34.3",
    "steiger": "^0.5.9",
    "svgo": "^4.0.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-plugin-minify": "^2.1.0",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
