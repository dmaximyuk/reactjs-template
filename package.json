{
  "name": "template",
  "description": "A simple template for everyone!",
  "homepage": ".",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "author": {
    "name": "Dmitry Maximyuk",
    "email": "d_maksimyk@vk.com",
    "url": "https://github.com/dmaximyuk"
  },
  "keywords": [
    "template",
    "react",
    "@tanstack",
    "typescript",
    "javascript",
    "sass",
    "swc",
    "fsd"
  ],
  "engines": {
    "node": ">=22.14"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:compress": "vite build --mode compress",
    "build:analyzer": "vite build --mode analyzer",
    "serve": "vite preview",
    "deploy": "gh-pages -d ./dist",
    "update": "ncu -u && npm install",
    "postinstall": "husky",
    "prepare": "husky",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.127.8",
    "@tanstack/react-router-devtools": "^1.127.8",
    "@tanstack/react-store": "^0.7.3",
    "clsx": "^2.1.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "ky": "^1.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^15.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tanstack/router-plugin": "^1.127.8",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.1.2",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
